{
  "name": "all-object-keys-deep",
  "version": "2.2.2",
  "description": "recursive get all keys of object",
  "main": "./lib/all-object-keys-deep.js",
  "directories": {
    "test": "test"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "scripts": {
    "init": "mkdirp dist",
    "test": "tape test/**/*.js",
    "coverage": "nyc npm test",
    "report": "nyc report --reporter=text-lcov | coveralls",
    "watch:test": "npm run watcher -- npm test",
    "watch:tape": "nodemon -w test -w lib --exec tape",
    "watch:coverage:tape": "npm run watcher -- nyc tape",
    "watch:coverage": "npm run watcher -- nyc npm test",
    "watcher": "nodemon -w test -w lib --exec",
    "6to5:lib": "babel lib -d legacy/lib",
    "build": "redrun init 6to5:* legacy:* bundle:* minify",
    "wisdom": "npm run build",
    "legacy:index": "echo \"module.exports = require('./lib/all-object-keys-deep');\" > legacy/index.js",
    "lint:eslint": "eslint lib test",
    "lint": "redrun lint:*",
    "fixlint": "npm run lint:eslint -- --fix",
    "bundle": "browserify -s allObjectKeysDeep --ng false",
    "bundle-es6": "npm run bundle -- -r ./lib/all-object-keys-deep.js:all-object-keys-deep ./lib/all-object-keys-deep.js",
    "bundle:es5": "npm run bundle -- -r ./legacy/lib/all-object-keys-deep.js:all-object-keys-deep ./legacy/lib/all-object-keys-deep.js -o dist/all-object-keys-deep.js",
    "bundle:es6": "npm run bundle-es6 -- -o dist/all-object-keys-deep.es6.js",
    "minify": "minify dist/all-object-keys-deep.js > dist/all-object-keys-deep.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/padurets/all-object-keys-deep.git"
  },
  "keywords": [
    "json",
    "object",
    "key",
    "nested",
    "get"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/padurets/all-object-keys-deep/issues"
  },
  "homepage": "https://github.com/padurets/all-object-keys-deep#readme",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "browserify": "^13.1.0",
    "coveralls": "^2.11.13",
    "eslint": "^3.5.0",
    "minify": "^2.0.11",
    "mkdirp": "^0.5.1",
    "nodemon": "^1.10.2",
    "nyc": "^10.0.0",
    "redrun": "^5.7.0",
    "tape": "^4.6.0"
  }
}
